<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.google.android.material.search.SearchBar
            android:id="@+id/searchbar_wilayah"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Cari Wilayah"
            android:layout_marginTop="10dp"
            android:layout_marginStart="30dp"
            android:layout_marginEnd="30dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@id/loc_makassar"/>

        <TextView
            android:id="@+id/loc_makassar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Makassar"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@id/searchbar_wilayah"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toTopOf="@id/long_line"/>

        <View
            android:id="@+id/long_line"
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray"
            app:layout_goneMarginTop="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            app:layout_constraintTop_toBottomOf="@id/loc_makassar"
            app:layout_constraintBottom_toTopOf="@id/loc_barru"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>

        <TextView
            android:id="@+id/loc_barru"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Barru"
            android:layout_marginTop="10dp"
            app:layout_constraintStart_toStartOf="@id/long_line"
            app:layout_constraintTop_toBottomOf="@id/long_line"
            app:layout_constraintBottom_toTopOf="@id/loc_barru"/>

        <View
            android:id="@+id/long_line2"
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray"
            app:layout_goneMarginTop="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            app:layout_constraintTop_toBottomOf="@id/loc_barru"
            app:layout_constraintBottom_toTopOf="@id/loc_sinjai"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>

        <TextView
            android:id="@+id/loc_sinjai"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Sinjai"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@id/long_line2"
            app:layout_constraintStart_toStartOf="parent"/>

        <View
            android:id="@+id/long_line3"
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray"
            app:layout_goneMarginTop="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            app:layout_constraintTop_toBottomOf="@id/loc_sinjai"
            app:layout_constraintBottom_toTopOf="@id/loc_Bulukumba"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>


        <TextView
            android:id="@+id/loc_Bulukumba"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Bulukumba"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@id/long_line3"
            app:layout_constraintStart_toStartOf="parent"/>

        <View
            android:id="@+id/long_line4"
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray"
            app:layout_goneMarginTop="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            app:layout_constraintTop_toBottomOf="@id/loc_Bulukumba"
            app:layout_constraintBottom_toTopOf="@id/loc_Bone"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>

        <TextView
            android:id="@+id/loc_Bone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Bone"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@id/long_line4"
            app:layout_constraintStart_toStartOf="parent"/>

        <View
            android:id="@+id/long_line5"
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray"
            app:layout_goneMarginTop="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            app:layout_constraintTop_toBottomOf="@id/loc_Bone"
            app:layout_constraintBottom_toTopOf="@id/loc_Enrekang"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>

        <TextView
            android:id="@+id/loc_Enrekang"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Enrekang"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            app:layout_constraintTop_toBottomOf="@id/long_line5"
            app:layout_constraintStart_toStartOf="parent"/>


    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.cardview.widget.CardView>